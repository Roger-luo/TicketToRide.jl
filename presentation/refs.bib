  file = {/home/mbeach/Zotero/storage/EM92M37T/Hu_et_al_2017_Discovering_phases,_phase_transitions,_and_crossovers_through_unsupervised.pdf;/home/mbeach/Zotero/storage/LT46E8YD/Hu_et_al_2017_Discovering_phases,_phase_transitions,_and_crossovers_through_unsupervised.pdf;/home/mbeach/Zotero/storage/PP2NP247/PhysRevE.95.html;/home/mbeach/Zotero/storage/YVZEPYB3/PhysRevE.95.html},
